{
    "rules": {
        "indent": [
            1
        ],
        "quotes": [
            2,
            "double"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "react/jsx-indent-props": 1,
        "react/jsx-indent": 1,
        "class-property/rule-name": 2
    },
    "env": {
        "browser": true
    },
    "extends": "eslint:recommended",
    // "ecmaFeatures": {
    //     "jsx": true,
    //     "experimentalObjectRestSpread": true
    // },
    "parserOptions": { "ecmaVersion": 6 },
    "parser": "babel-eslint",
    "rules": {
        "strict": 0
    },
    "plugins": [
        "react",
        "class-property"
    ]
}