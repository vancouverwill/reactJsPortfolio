{
    "rules": {
        "indent": [
            1
        ],
        "quotes": [
            2,
            "double"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ]
    },
    "env": {
        "browser": true
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
    },
    "parserOptions": { "ecmaVersion": 6 },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-indent-props": 1,
        "react/jsx-indent": 1
    }
}